[odata://default]
*The stanza name must be unique

address =
* The uri for the odata web service

tailFilterProperty =
* The name of a property which will be pulled from the last result and injected into the filter

defaultTailFilter =
* A default value to be injected as the tail filter value for the first run

resource =
* The name of the resources which will be retrieved

filter =
* The OData query as a filter string
